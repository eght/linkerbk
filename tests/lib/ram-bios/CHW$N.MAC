;; vim: set expandtab ai :
;; Обмен одного слова
;; R0 --- слово для обмена.
;; ADR --- адрес
;; MODE$FLAGS --- режим + флаги (100000 -- ``любой'' режим или массив в основном
;; ОЗУ/ПЗУ, 40000 -- устанавливать, если в этом режиме отсутствует ПЗУ ROM-BIOS)
;; код режима 0 означает текущий ружим.
                .MACRO  CHW$N ADR,MODE$FLAGS
                JSR     R5,@#167525
                .WORD   ADR, MODE$FLAGS
                .ENDM

